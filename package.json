{
  "name": "multilateration",
  "version": "0.5.1",
  "description": "Locate points according to distributed beacon signals with consistent propagation speeds",
  "license": "MIT",
  "unpkg": "dist/multilateration.min.js",
  "jsdelivr": "dist/multilateration.min.js",
  "main": "dist/multilateration.common.js",
  "module": "dist/multilateration.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/multilateration.git"
  },
  "homepage": "https://github.com/vasturiano/multilateration",
  "keywords": [
    "multilateration",
    "geolocation",
    "positioning",
    "least square errors",
    "minimize"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/multilateration/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/multilateration.js -o dist/multilateration.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "dependencies": {
    "fmin": "^0.0.2",
    "venn.js": "^0.2.20"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
